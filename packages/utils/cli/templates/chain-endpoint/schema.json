{
    "$schema": "http://json-schema.org/schema",
    "id": "chain-endpoint",
    "type": "object",
    "path": "./packages/utils/cli/templates/chain-endpoint/template",
    "outputPath": "./apps/dashboard/app/api/chat/PARENT",
    "properties": {
      "parent": {
        "type": "string",
        "description": "Typeof chain (assistants or generators)",
        "enum": ["assistants", "generators"],
        "default": "assistants"
      },
      "name": {
        "type": "string",
        "description": "Feature name"
      }
    },
    "required": ["name", "parent"]
}