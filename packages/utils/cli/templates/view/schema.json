{
  "$schema": "http://json-schema.org/schema",
  "id": "view-generator",
  "type": "object",
  "path": "./packages/libs/novecirculos-generator/templates/view/template",
  "outputPath": "./packages/features/PARENT/NAME",
  "properties": {
    "name": {
      "type": "string",
      "description": "View name"
    },
    "parent": {
      "type": "string",
      "description": "Parent folder eg: StockAnalysis",
      "default": "Common"
    }
  },
  "required": ["name"]
}