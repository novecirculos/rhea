{
    "$schema": "http://json-schema.org/schema",
    "id": "network-story-generator",
    "type": "object",
    "path": "./packages/libs/novecirculos-generator/templates/network-story/template",
    "outputPath": "./apps/storybook/stories/Network",
    "install": [
      {
        "dependencies": {
          "@novecirculos/NAME-network": "*"
        },
        "targets": ["./apps/storybook/package.json"]
      }
    ],
    "properties": {
      "name": {
        "type": "string",
        "description": "Feature name"
      }
    },
    "required": ["name"]
}