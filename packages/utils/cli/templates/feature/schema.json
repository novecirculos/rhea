{
    "$schema": "http://json-schema.org/schema",
    "id": "feature-generator",
    "type": "object",
    "path": "./packages/libs/novecirculos-generator/templates/feature/template",
    "outputPath": "./packages/features/PARENT/NAME",
    "properties": {
      "name": {
        "type": "string",
        "description": "Feature name"
      },
      "parent": {
        "type": "string",
        "description": "Parent folder eg: StockAnalysis",
        "default": "Common"
      }
    },
    "required": ["name", "parent"],
    "dependsOn": ["model", "view", "view-model", "feature-story", "view-story"]
}