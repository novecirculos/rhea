{
    "$schema": "http://json-schema.org/schema",
    "id": "network-story-generator",
    "type": "object",
    "path": "./packages/libs/novecirculos-generator/templates/query-with-vars-story/template",
    "outputPath": "./apps/storybook/stories/Network/PARENT-KEBAB-network-story",
    "install": [
      {
        "dependencies": {
          "@novecirculos/PARENT-KEBAB-network": "*"
        },
        "targets": ["./apps/storybook/package.json"]
      }
    ],
    "properties": {
      "name": {
        "type": "string",
        "description": "Query name"
      },
      "parent": {
        "type": "string",
        "description": "Parent Network Lib name",
        "default": "midasTouch"
      },
      "variables": {
        "type": "string",
        "description": "Comma-sepaprated variables to be used in query (e.g. 'username,password')",
        "default": "region"
      }
    },
    "required": ["name", "parent"],
    "spread": true
}