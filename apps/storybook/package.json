{
  "name": "@novecirculos/storybook",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "yarn build:css && yarn build:storybook",
    "build:css": "npx tailwindcss -i src/styles/global.css -o ./public/tailwind.css --postcss postcss.config.js",
    "build:storybook": "build-storybook -s ./public",
    "deploy-storybook": "storybook-to-ghpages",
    "dev": "yarn build:css && concurrently \"yarn:watch:*\"",
    "dev:ci": "concurrently \"yarn:watch:ci:*\"",
    "start": "yarn watch:storybook",
    "watch:storybook": "start-storybook -p 6006 --no-manager-cache -s ./public",
    "watch:css": "yarn build:css --watch",
    "watch:ci:storybook": "yarn:watch:storybook --ci",
    "watch:ci:css": "yarn:watch:css",
    "test-storybook": "test-storybook"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-docs": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/addon-react-native-web": "^0.0.19",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/react": "^6.5.14",
    "@storybook/test-runner": "^0.9.2",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/builder-vite": "^0.2.3",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^7.6.0",
    "react-dom": "^18.2.0",
    "react-native-web": "~0.18.10",
    "storybook-addon-themes": "^6.1.0",
    "tailwindcss": "^3.2.7",
    "wait-on": "^7.0.1",
    "@babel/core": "^7.19.3",
    "babel-loader": "^8.2.5",
    "postcss": "^8.4.21",
    "vite": "^3.1.4"
  },
  "dependencies": {
    "@novecirculos/eslint-config": "*",
    "@novecirculos/react": "*",
    "@novecirculos/tokens": "*",
    "@novecirculos/ts-config": "*",
    "@storybook/addon-a11y": "^6.5.13",
    "@storybook/storybook-deployer": "^2.8.16",
    "nativewind": "^2.0.11",
    "phosphor-react": "^1.4.1",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
